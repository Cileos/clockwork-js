thead
  tr
    th Teams
    each x in structure.formattedXValues
      th
        = x
tbody
  each row in structure.sortedRows
    tr
      th= row.formattedValue
      each cell in row.cells
        td: ul
          each item in cell.itemsByTime
            li.scheduling
              abbr.work_time title=item.shortPeriod: span= item.shortPeriod
